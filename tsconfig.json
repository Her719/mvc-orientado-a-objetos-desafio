{
  "extends": "@sindresorhus/tsconfig",
  "compilerOptions": {
    "noUnusedLocals": false,
    "module": "commonjs",
    "outDir": "build",
    "resolveJsonModule": true,
    "noUnusedParameters": false,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "target": "esnext",
    "moduleResolution": "node"
  }
}
